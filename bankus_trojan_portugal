import "pe"

rule Bankus_trojan_Portugal {
  meta:
  description = "Yara rule for Bankus Trojan Portugal - May version"
  author = "SI-LAB @sirpedrotavares - https://seguranca-informatica.pt"
  last_updated = "2020-05-05"
  tlp = "white"
  category = "informational"

  strings:
    $bankus_a = {42 61 6E 63 6F 42 70 69}
    $bankus_b = {4D 69 6C 6C 65 6E 69 75 6D 42 63 70}

  condition:
    all of ($bankus_*) and pe.number_of_sections > 6
}
